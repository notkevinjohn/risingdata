export LM_LICENSE_FILE=${IDL_DIR}/license/license.dat
export IDL_PATH=+$IDL_DIR/lib
export IDL_PATH=$IDL_PATH:+'~/IDLWorkspace83/'
export IDL_DEVICE=X
alias idl='${IDL_DIR}/bin/idl'



